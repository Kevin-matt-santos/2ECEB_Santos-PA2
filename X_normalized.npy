       "       [-0.97536604,  1.07442092, -0.77140813,  1.01490811,  0.7876157 ],\n",
       "       [ 1.42521159, -1.5549174 , -1.08550176,  0.51353225,  0.48478045],\n",
       "       [-0.91447906,  0.89906097,  0.22663844, -1.275065  ,  0.25050569]])"
      ]
     },
     "execution_count": 13,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#Making an 5x5 array with random values inside the variable X\n",
    "X = np.random.random((5,5))\n",
    "\n",
    "#Finding the square root of each values in the array X\n",
    "f = np.std(X)\n",
    "\n",
    "# Finding the mean of all the values inside the array\n",
    "v = np.mean(X)\n",
    "\n",
    "#Formula for Normalization, used on each value in the array\n",
    "z = (X - v) / f\n",
    "\n",
    "#Print the final values after the Normalization solution\n",
    "z"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "bdfc8218-c0ee-4c5c-bbef-7799e97955a1",
   "metadata": {},
   "outputs": [],
   "source": [
    "#save array Z using the file name X_normalized.npy\n",
    "np.save('X_normalized.npy', z)"
   ]
  },
